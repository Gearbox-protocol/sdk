{
  "name": "@gearbox-protocol/sdk",
  "version": "0.0.0-semantic-release",
  "description": "Gearbox SDK",
  "license": "MIT",
  "main": "./dist/cjs/sdk/index.cjs",
  "module": "./dist/esm/sdk/index.mjs",
  "types": "./dist/cjs/sdk/index.d.ts",
  "typings": "./dist/cjs/sdk/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "default": {
        "types": "./dist/cjs/sdk/index.d.ts",
        "default": "./dist/cjs/sdk/index.cjs"
      },
      "import": {
        "types": "./dist/esm/sdk/index.d.mts",
        "default": "./dist/esm/sdk/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/sdk/index.d.ts",
        "default": "./dist/cjs/sdk/index.cjs"
      }
    },
    "./dev": {
      "default": {
        "types": "./dist/cjs/dev/index.d.ts",
        "default": "./dist/cjs/dev/index.cjs"
      },
      "import": {
        "types": "./dist/esm/dev/index.d.mts",
        "default": "./dist/esm/dev/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/dev/index.d.ts",
        "default": "./dist/cjs/dev/index.cjs"
      }
    },
    "./adapters": {
      "default": {
        "types": "./dist/cjs/adapters/index.d.ts",
        "default": "./dist/cjs/adapters/index.cjs"
      },
      "import": {
        "types": "./dist/esm/adapters/index.d.mts",
        "default": "./dist/esm/adapters/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/adapters/index.d.ts",
        "default": "./dist/cjs/adapters/index.cjs"
      }
    },
    "./abi/*": {
      "default": {
        "types": "./dist/cjs/abi/*.d.ts",
        "default": "./dist/cjs/abi/*.cjs"
      },
      "import": {
        "types": "./dist/esm/abi/*.d.mts",
        "default": "./dist/esm/abi/*.mjs"
      },
      "require": {
        "types": "./dist/cjs/abi/*.d.ts",
        "default": "./dist/cjs/abi/*.cjs"
      }
    }
  },
  "files": [
    "*"
  ],
  "typesVersions": {
    "*": {
      "dev": [
        "./dist/cjs/dev/index.d.ts"
      ],
      "adapters": [
        "./dist/cjs/adapters/index.d.ts"
      ],
      "abi/*": [
        "./dist/cjs/abi/*.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "example": "tsx --env-file .env scripts/example.ts | pino-pretty",
    "accounts": "tsx --env-file .env scripts/accounts.ts | pino-pretty",
    "test": "yarn jest",
    "prepare": "husky",
    "prettier": "prettier --write .",
    "prettier:ci": "npx prettier --check .",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:ci": "eslint \"**/*.ts\"",
    "typecheck:ci": "tsc --noEmit"
  },
  "dependencies": {
    "@redstone-finance/evm-connector": "^0.7.3",
    "@redstone-finance/protocol": "^0.7.3",
    "abitype": "^1.0.8",
    "date-fns": "^4.1.0",
    "decimal.js-light": "^2.5.1",
    "viem": ">=2.21.0 <3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@gearbox-protocol/eslint-config": "^2.0.0-next.2",
    "@gearbox-protocol/prettier-config": "^2.0.0",
    "@jest/globals": "^29.7.0",
    "@types/cross-spawn": "^6.0.6",
    "axios": "^1.8.1",
    "cross-spawn": "^7.0.6",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "axios": "^1.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@gearbox-protocol/eslint-config"
    ]
  },
  "prettier": "@gearbox-protocol/prettier-config",
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "packageManager": "yarn@4.6.0"
}
