{
  "name": "@gearbox-protocol/sdk",
  "version": "0.0.0-semantic-release",
  "description": "Gearbox SDK",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "contracts"
  ],
  "author": "Mikael Lazarev",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc --p tsconfig.build.json",
    "dev": "tsc -w --p tsconfig.build.json",
    "types:clean": "rm -rf forge-out src/types",
    "types:build": "forge build",
    "types:split": "ts-node ./scripts/split-index.ts",
    "types:index": "ts-node ./scripts/create-index.ts",
    "types": "yarn types:clean && yarn types:build && npx wagmi generate && yarn types:split && yarn types:index",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "prettier:ci": "npx prettier --check .",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:ci": "eslint \"**/*.ts\"",
    "typecheck:ci": "tsc --noEmit",
    "test": "npx mocha -r ts-node/register -r dotenv/config 'src/**/*.spec.ts'"
  },
  "dependencies": {
    "@gearbox-protocol/sdk-gov": "^2.36.6",
    "@wagmi/cli": "^2.1.13",
    "axios": "^1.2.6",
    "decimal.js-light": "^2.5.1",
    "deep-eql": "^4.1.0"
  },
  "devDependencies": {
    "@1inch/farming": "^3.1.0",
    "@1inch/solidity-utils": "^2.2.27",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@gearbox-protocol/bots-v3": "^1.5.1",
    "@gearbox-protocol/core-v2": "1.19.0-base.17",
    "@gearbox-protocol/core-v3": "^1.51.0",
    "@gearbox-protocol/eslint-config": "^2.0.0-next.2",
    "@gearbox-protocol/integrations-v3": "^1.44.0",
    "@gearbox-protocol/oracles-v3": "^1.13.0",
    "@gearbox-protocol/periphery-v3": "^1.6.1",
    "@gearbox-protocol/prettier-config": "^2.0.0",
    "@gearbox-protocol/router": "^1.5.5",
    "@gearbox-protocol/router-v3": "^1.41.0",
    "@openzeppelin/contracts": "^4.9.3",
    "@redstone-finance/evm-connector": "^0.7.3",
    "@types/chai": "^4.3.3",
    "@types/deep-eql": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.7.6",
    "@types/rimraf": "^3.0.2",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.57.1",
    "husky": ">=6",
    "jest": "^29.7.0",
    "lint-staged": "^13.2.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tslog": "^3.3.3",
    "typechain": "^8.1.0",
    "typescript": "5.0.4",
    "viem": "^2.21.57"
  },
  "prettier": "@gearbox-protocol/prettier-config",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "jest": {
    "rootDir": "src"
  }
}
